<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox minWidth="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seedu.address.ui.MainWindow">

         <stylesheets>
      <URL value="@DarkTheme.css" />
      <URL value="@Extensions.css" />
   </stylesheets>
   <children>
      <MenuBar minHeight="30.0" prefHeight="30.0" styleClass="anchor-pane" VBox.vgrow="NEVER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane fx:id="currentTimePanelPlaceholder" minHeight="15.0" prefHeight="15.0" VBox.vgrow="NEVER" />
      <AnchorPane VBox.vgrow="NEVER">
         <children>
            <AnchorPane fx:id="taskscopePlaceholder" minHeight="30.0" prefHeight="30.0">
               <children>
                  <Label layoutX="360.0" layoutY="6.0" text="Task Scope" />
               </children>
            </AnchorPane>
         </children>
      </AnchorPane>
      <AnchorPane maxHeight="20.0" minHeight="20.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="20.0" styleClass="anchor-pane" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
         <children>
            <Label layoutX="18.0" layoutY="2.0" text="Task" AnchorPane.leftAnchor="200.0" />
            <Label text="Start" AnchorPane.rightAnchor="310.0" />
            <Label text="End" AnchorPane.rightAnchor="180.0" />
            <Label text="Hashtag" AnchorPane.rightAnchor="50.0" />
         </children>
      </AnchorPane>
      <VBox fx:id="personList">
         <children>
            <AnchorPane fx:id="personListPanelPlaceholder" VBox.vgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </VBox>
      <AnchorPane fx:id="commandBoxPlaceholder" maxHeight="50.0" minHeight="50.0" prefHeight="50.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </AnchorPane>
       <AnchorPane fx:id="resultDisplayPlaceholder" maxHeight="50.0" minHeight="50.0" prefHeight="50.0" styleClass="anchor-pane-with-border" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
       </AnchorPane>
   </children>
</VBox>
